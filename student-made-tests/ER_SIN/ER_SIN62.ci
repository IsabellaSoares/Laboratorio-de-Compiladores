@cep(11, "'}' inesperado antes de 'until'", 
  "'}' not expected before 'until'", 
  "comp.Compiler.repeatStat()")

class Program

    func run {
        var Int i;
        i = 0;
        repeat
            i = i + 1;
        }
        until (i < 10); 
    }

end
