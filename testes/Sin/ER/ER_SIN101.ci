@cep(2, "missing type in variable declaration", "Type expected")
class Teste
    var n;

    func get -> Int {
        return self.n;
    }

    func set: Int n {
        self.n = n;
    }
end

class Program
    func run {
        var Teste t;
        var Teste a;
        t = Teste.new;
        a = In.readInt;
        t.set: a;
        Out.println: t.get;
    }
end
