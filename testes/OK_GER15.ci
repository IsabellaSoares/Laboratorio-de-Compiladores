open
class A
    var Int i
    var Int j
    
    private func p {
        Out.print: self.i, " ";
    }    
    
    private func q {
        Out.print: self.j, " ";
    }    
    
    func init_A {
        self.i = 1;
        self.j = 2;
    }  
    
    func call_p {
        self.p;
    }    
    
    func call_q {
        self.q;
    }    
    
    func r {
        Out.print: self.i, " ";
    }    
    
    func s {
        Out.print: self.j, " ";
    }    
end


