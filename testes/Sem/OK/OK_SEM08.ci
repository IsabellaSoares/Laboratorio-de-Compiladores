@annot("check", "methodOverridingSameSignature")
@nce

/*
  @filename   ok-sem08.ci

  @comment    Testa se o compilador aceita redefinicoes de metodos com tipos dos
  parametros iguais mas nomes diferentes.
  */

open
class A 
   
   private var Int i;
   
   public func put: Int x, Int y, Boolean ok {
    
      if (x > y) && ok {
         self.i = 0;
      }
      
   }
   
   public func get -> Int {
      return self.i;
   }   
   
   public func set: Int i {
      self.i = i;
   }   

end


class B extends A 

   override   
   public func put: Int x, Int y, Boolean ok {
    
      if ( (x + y) < 1) && !ok  {
         Out.print: 0;
      }
   }   

end


class Program 
   
   func run {
      var B b;
      
      b = B.new;
      b.put: 1, 2, true;
   }   

end

