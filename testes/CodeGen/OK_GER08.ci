/*
  @filename   ok-ger08.ci

  @comment    Testa se a passagem de parametros e a chamada de metodos
  estao corretas.
  Deve escrever
    1 1 2 2 3 3

  */

class A
    func m1: Int n {
        Out.print: 1, " ";
        Out.print: n, " ";
    }   
   
    func m2: Int n {
        Out.print: 2, " ";
        Out.print: n, " ";
    }   
   
    func m3: Int n, Int p, String q, Int r, Boolean falseBool {
        Out.print: 3, " ";
        Out.print: n, " ";
        Out.print: p, " ";
        Out.print: q, " ";
        Out.print: r, " ";
        if falseBool {
            Out.print: 8 ++ " ";
        }
        else {
            Out.print: 7 ++ " ";
        }
    }   
end


class Program
    func run {
        var A a;

        Out.println: "1 1 2 2 3 3 4 5 6 7";
        a = A.new;
        a.m1: 1;
        a.m2: 2;
        a.m3: 3, 4, "5", 6, false;
    }
end
