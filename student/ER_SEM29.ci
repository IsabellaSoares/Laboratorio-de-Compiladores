@annot("check", "wrongSubclassMethodSignature")
@cep(15, "'Método 'put' redefinido na classe 'B' com parâmetros diferentes daqueles da superclasse 'A'", 
  "Method 'put' of the subclass 'B' has a signature different from the same method of superclass 'A'",
  "comp.Compiler.methodDec() or comp.Compiler.classDec()")

open
class A 
    
    func put: Int x, Int y {
    }   

end


class B extends A 
    override
    func put: Int x {
    }   
end


class Program 
   
    func run {
        var A a;
      
        a = A.new;
        a.put;
    }   

end


